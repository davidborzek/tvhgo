    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^7.0.0",
    "react-toastify": "^11.0.0",
    "react-top-loading-bar": "^3.0.0",
    "ua-parser-js": "^1.0.35",
    "yup": "^1.0.0"
